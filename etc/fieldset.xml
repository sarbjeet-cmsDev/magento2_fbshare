<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DataObject/etc/fieldset.xsd">
    <scope id="global">
        <fieldset id="sales_convert_quote">
            <field name="fbdiscount">
                <aspect name="to_order" />
            </field>
            <field name="fbuserid">
                <aspect name="to_order" />
            </field>
        </fieldset>
        <fieldset id="quote_convert_item">
            <field name="fbdiscount">
                <aspect name="to_order_item" />
            </field>
        </fieldset>
        <fieldset id="sales_convert_order">
            <field name="fbdiscount">
                <aspect name="to_invoice" />
                <aspect name="to_cm" />
                <aspect name="to_shipment" />
            </field>
        </fieldset>
    </scope>
</config>